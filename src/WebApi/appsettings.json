{
  "AllowedHosts": "*",
  "Consul": {
    "Address": "http://localhost:8500",
    "Environment": "Development",
    "ServiceName": "Phoenix",
    "ServiceAddress": "localhost",
    "ServicePort": 5080,
    "ServiceTags": [
      "WebApi"
    ],
    "ServiceHealthCheckAddress": "http://localhost:5080/health"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Metrics": {
    "EnabledMetrics": {
      "Microsoft.AspNetCore.*": true,
      "System.*": true,
      "Phoenix.*": true
    }
  }
}